[GLOBAL]
#******************************************************************************#
#* Description: Config file version. Do not modify
#* Default Value: 10.5.200
#******************************************************************************#
VERSION = 10.5.200

[SETTINGS]
#******************************************************************************#
#* Description: The port on which the LVManager listens to for requests from LVDashboard
#*
#* Default Value: 7589
#******************************************************************************#
ManagerPort = default

#******************************************************************************#
#* Description: Interval between scans (in milliseconds)
#*
#* Default Value: 1000
#******************************************************************************#
Interval = default

#******************************************************************************#
#* Description: Scan Priority:
#*    0=Low CPU usage
#*    1=Normal CPU use
#*    2=Above normal CPU use
#* Default Value: 0
#******************************************************************************#
ScanPriority = default

#******************************************************************************#
#* Description: Authentication mode:
1=Username/Password requiredquired
#* Default Value: 0
#******************************************************************************#
EnableAuthentication = default

#******************************************************************************#
#* Description: When authentication mode is enabled, a username with this value is required for remote access to the LVManager
#* Default Value: LVAdmin
#******************************************************************************#
Username = default

#******************************************************************************#
#* Description: When authentication mode is enabled, a password with this value is required for remote access to the LVManager
#* Default Value: LVPassword
#******************************************************************************#
Password = default

[LICENSE_SERVER]
#******************************************************************************#
#* Description: The maintenance port to connect to on the License Server
#* Default Value: 7569
#******************************************************************************#
MaintenancePort = default

[SRE_SERVER]
#******************************************************************************#
#* Description: The maintenance port to connect to on the SRE Server
#* Default Value: 5730
#******************************************************************************#
MaintenancePort = default

[TTS_SERVER]
#******************************************************************************#
#* Description: The maintenance port to connect to on the TTS Server
#* Default Value: 7579
#******************************************************************************#
MaintenancePort = default

[MEDIA_SERVER]
#******************************************************************************#
#* Description: The maintenance port to connect to on the Media Server
#* Default Value: 7590
#******************************************************************************#
MaintenancePort = default

[CALL_INDEXER]
#******************************************************************************#
#* Description: The maintenance port to connect to on the CallIndexer
#* Default Value: 7595
#******************************************************************************#
MaintenancePort = default
